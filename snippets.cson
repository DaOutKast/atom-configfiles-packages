# JS SNIPPETS
'.source.js':
    'isEven':
      'prefix': 'isEven'
      'body': """
        var isEven = function(number) {
          if(number % 2 === 0){
            return true;
          } else if (isNaN(number)) {
            return "This is not a number;"
          } else {
            return false;
          }
        };
      """
#---------------------------------------------------------------------------------------#
#---------------------------------------------------------------------------------------#
    'JS EVENT KEY CODES':
      'prefix': 'js-keycode'
      'body': """
        window.addEventListener('keydown', function(e) {
          console.log(e.keyCode);
        });
      """
