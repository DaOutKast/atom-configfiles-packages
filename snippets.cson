'.text.html.basic':
    'Mailer Table tr td':
      'prefix': 'table-mailer'
      'body': """
      <table align="center" cellpadding="0" cellspacing="0" border="0" width="$1" style="width: $2; border-spacing:0px; mso-table-lspace: 0pt;mso-table-rspace: 0pt; border: none;" class="$3">$0
       <tr>
        <td valign="top">

        </td>
       </tr>
      </table>
        """
