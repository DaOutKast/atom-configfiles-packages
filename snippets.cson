    'Mailer Layout':
      'prefix': 'layout-mailer'
      'body': """
         <!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
         <html xmlns="http://www.w3.org/1999/xhtml">
         <head>
          <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
          <meta name="viewport" content="width=device-width, initial-scale=1.0" />
          <title>Subject line</title>
          <!-- Outlook --><!--[if gte mso 15]>
          <style type="text/css">
          table { font-size:1px; line-height:0; mso-margin-top-alt:1px; }
         </style>
         <![endif]--><!--[if gte mso 9]>
         <style>
         table{
         border-collapse: collapse;
         mso-table-lspace: 0pt;
         mso-table-rspace: 0pt;
         }
         table td{
         border-collapse: collapse;
         }
         .wrapper table{
         border-collapse: collapse;
         mso-table-lspace: 0pt;
         mso-table-rspace: 0pt;
         }
         .wrapper table td{
         border-collapse: collapse;
         }
         </style>
         <![endif]-->
         <style type="text/css">#outlook a {padding:0;}
         body, #yiv2200947006 body{width:100% !important; -webkit-text-size-adjust:100%; -ms-text-size-adjust:100%; margin:0; padding:0;}
         .ExternalClass {width:100%;}
         .ExternalClass, .ExternalClass a, .ExternalClass p, .ExternalClass span, .ExternalClass font, .ExternalClass td, .ExternalClass div {line-height: 100%;}
         #backgroundTable {margin:0; padding:0; width:100% !important; line-height: 100% !important;}
         img {outline:none; text-decoration:none; -ms-interpolation-mode: bicubic;}
         a img { border:none; display: block; }
         .image_fix {display:block;}
         p {margin: 1em 0;}
         table td {border-collapse: collapse;}
         table{
          border-collapse: collapse;
          mso-table-lspace: 0pt;
          mso-table-rspace: 0pt;
         }
         table td{
          border-collapse: collapse;
          border: 0;
         }
         p {margin: 0 !important;}
         .ExternalClass p  {margin: 0;}

         /*Mobile Breakpoint*/
         @media screen and (max-width: 480px) {
          .body {
           margin: 0;
           padding: 0;
          }
          .email-wrapper {
           width: 320px !important;
          }
          .content-wrapper {
           width: 280px !important;
          }
          /*Copy*/
          .body-copy-padding {
           padding-left: 25px !important;
           padding-right: 25px !important;
          }
          /*Copy End*/
          .padding-remove {
           padding-top: 0 !important;
           padding-right: 0 !important;
           padding-bottom: 0 !important;
           padding-left: 0 !important;
          }
          .mobile-swap {
           mso-hide:none;
           max-height: 100% !important;
           width: 100% !important;
           height: auto;
           display: block  !important;
           overflow: hidden;
          }
          .res-hide {
           display: none;
           display: none !important;
          }
          /*Gmail Fix*/
          .gmailfix {
           display: none;
           display: none !important;
          }
          /*Gmail Fix End*/
         }
         /*End Mobile Breakpoint*/
         </style>
         </head>
         <body style="margin: 0; padding: 0; width: 100%; background-color: #ffffff;" class="body">
          <table align="center" cellpadding="0" cellspacing="0" border="0" style="width: 100%; border-spacing:0px; mso-table-lspace: 0pt;mso-table-rspace: 0pt; border: none; background-color:#ffffff;" class="email-wrapper">
           <tr>
            <td valign="top">
             <table align="center" cellpadding="0" cellspacing="0" border="0" width="600" style="width: 600px; border-spacing:0px; mso-table-lspace: 0pt;mso-table-rspace: 0pt; border: none;" class="email-wrapper">
              <tr>
               <td valign="top">
                $1
               </td>
              </tr>
             </table>
            </td>
           </tr>
          </table>
          <!-- GMAIL FIXES -->
           <div class="gmailfix" style="white-space:nowrap; font:15px courier; line-height:0;">
            &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
            &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
            &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
           </div>
           <!-- <div class=”gmailfix” style="white-space:nowrap; font:15px courier; color:#ffffff;">
            - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
           </div> -->
           <!-- GMAIL FIXES END -->
         </body>
         </html>
         """
